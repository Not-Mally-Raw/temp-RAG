      "rules": [
        {
          "rule_text": "Bends should be toleranced plus or minus one-half degree at a location adjacent to the bends.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "bend",
            "location": "near bend"
          },
          "dimensional_constraints": [
            "Bend angle tolerance: ±0.5 degrees"
          ],
          "relational_constraints": [
            "None"
          ]
        },
        {
          "rule_text": "For the ease of manufacturing, multiple bends on the same plane should occur in the same direction.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "bend",
            "location": "same plane"
          },
          "dimensional_constraints": [
            "None"
          ],
          "relational_constraints": [
            "Bends on same plane should align directionally"
          ]
        },
        {
          "rule_text": "Avoid large sheet metal parts with small bent flanges.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "flange",
            "location": "any"
          },
          "dimensional_constraints": [
            "None"
          ],
          "relational_constraints": [
            "Part size relative to flange size"
          ]
        },
        {
          "rule_text": "In low carbon steel sheet metal, the minimum radius of a bend should be one-half the material thickness or 0.80 mm, whichever is larger.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "low carbon steel",
            "process": "sheet metal fabrication",
            "feature": "bend",
            "location": "any"
          },
          "dimensional_constraints": [
            "Bend radius >= max(0.5 × material thickness, 0.80 mm)"
          ],
          "relational_constraints": [
            "None"
          ]
        },
        {
          "rule_text": "The minimum distance between two counterbores is eight times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "counterbore",
            "location": "between counterbores"
          },
          "dimensional_constraints": [
            "Distance >= 8 × material thickness"
          ],
          "relational_constraints": [
            "Distance between counterbore features"
          ]
        },
        {
          "rule_text": "The minimum distance from a counterbore to an edge is four times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "counterbore",
            "location": "near edge"
          },
          "dimensional_constraints": [
            "Distance >= 4 × material thickness"
          ],
          "relational_constraints": [
            "Distance between counterbore and edge"
          ]
        },
        {
          "rule_text": "The minimum distance from a counterbore to a bend is four times the material thickness plus the bend radius.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "counterbore",
            "location": "near bend"
          },
          "dimensional_constraints": [
            "Distance >= 4 × material thickness + bend radius"
          ],
          "relational_constraints": [
            "Distance between counterbore and bend"
          ]
        },
        {
          "rule_text": "The minimum spacing between adjacent holes shall be at least twice the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "hole",
            "location": "adjacent holes"
          },
          "dimensional_constraints": [
            "Spacing >= 2 × material thickness"
          ],
          "relational_constraints": [
            "Distance between hole features"
          ]
        },
        {
          "rule_text": "Countersunk holes should not be placed closer than three times the material thickness from an edge.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "countersink",
            "location": "near edge"
          },
          "dimensional_constraints": [
            "Distance >= 3 × material thickness"
          ],
          "relational_constraints": [
            "Distance between countersink and edge"
          ]
        },
        {
          "rule_text": "The depth of a counterbore should not exceed three times the hole diameter.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "counterbore",
            "location": "any"
          },
          "dimensional_constraints": [
            "Counterbore depth <= 3 × hole diameter"
          ],
          "relational_constraints": [
            "Depth relative to diameter"
          ]
        },
        {
          "rule_text": "Sharp internal corners should be avoided in sheet metal parts.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "corner",
            "location": "internal"
          },
          "dimensional_constraints": [
            "None"
          ],
          "relational_constraints": [
            "Corner geometry advisory"
          ]
        },
        {
          "rule_text": "The minimum internal corner radius should be equal to the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "corner",
            "location": "internal"
          },
          "dimensional_constraints": [
            "Corner radius >= material thickness"
          ],
          "relational_constraints": [
            "None"
          ]
        },
        {
          "rule_text": "Slots should have a minimum width equal to the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "slot",
            "location": "any"
          },
          "dimensional_constraints": [
            "Slot width >= material thickness"
          ],
          "relational_constraints": [
            "None"
          ]
        },
        {
          "rule_text": "Slots should have a minimum end radius equal to half the slot width.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "slot",
            "location": "ends"
          },
          "dimensional_constraints": [
            "End radius >= 0.5 × slot width"
          ],
          "relational_constraints": [
            "Radius relative to width"
          ]
        },
        {
          "rule_text": "Emboss depth should not exceed two times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "emboss",
            "location": "any"
          },
          "dimensional_constraints": [
            "Emboss depth <= 2 × material thickness"
          ],
          "relational_constraints": [
            "None"
          ]
        },
        {
          "rule_text": "The minimum distance between emboss features should be three times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "emboss",
            "location": "between embosses"
          },
          "dimensional_constraints": [
            "Distance >= 3 × material thickness"
          ],
          "relational_constraints": [
            "Distance between emboss features"
          ]
        },
        {
          "rule_text": "Holes located on a bend line should be avoided.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "hole",
            "location": "bend line"
          },
          "dimensional_constraints": [
            "None"
          ],
          "relational_constraints": [
            "Hole relative to bend line"
          ]
        },
        {
          "rule_text": "The minimum distance from a hole to a bend should be two times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "hole",
            "location": "near bend"
          },
          "dimensional_constraints": [
            "Distance >= 2 × material thickness"
          ],
          "relational_constraints": [
            "Distance between hole and bend"
          ]
        },
        {
          "rule_text": "Formed features should be oriented to avoid tool interference.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "formed feature",
            "location": "any"
          },
          "dimensional_constraints": [
            "None"
          ],
          "relational_constraints": [
            "Orientation relative to tooling"
          ]
        },
        {
          "rule_text": "Minimum flange height should be three times the material thickness.",
          "rule_type": "Sheetmetal",
          "applicability_constraints": {
            "material": "any",
            "process": "sheet metal fabrication",
            "feature": "flange",
            "location": "any"
          },
          "dimensional_constraints": [
            "Flange height >= 3 × material thickness"
          ],
          "relational_constraints": [
            "None"
          ]
        }
      ]
    }
  ]
